<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="QQStepView">
        <!-- 圆弧底色 -->
        <attr name="outerColor" format="color"/>
        <!-- 进度条圆弧部分颜色 -->
        <attr name="innerColor" format="color"/>
        <attr name="borderWidth" format="dimension"/>
        <attr name="stepTextSize" format="dimension"/>
        <attr name="stepTextColor" format="color"/>
    </declare-styleable>
    <declare-styleable name="ColorTrackTextView">
        <attr name="originalColor" format="color"/>
        <attr name="changeColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="ColorTrackTextViewJava">
        <attr name="cmChangeTextColor" format="color" />
        <attr name="cmOriginTextColor" format="color" />
    </declare-styleable>
    
    <declare-styleable name="Progressbar">
        <!-- 圆环边框宽度 -->
        <attr name="circleProgressBorderWidth" format="dimension"/>
        <!-- 圆环正常颜色 -->
        <attr name="circleProgressNormalColor" format="color"/>
        <!-- 进度条颜色 -->
        <attr name="circleProgressbarColor" format="color"/>
        <!-- 进度文字字体大小 -->
        <attr name="circleProgressTextSize" format="dimension"/>
        <!-- 进度文字字体颜色 -->
        <attr name="circleProgressTextColor" format="color"/>
        
    </declare-styleable>
    
    <declare-styleable name="RatingBar">
        <attr name="rb_drawable_normal" format="reference"/>
        <attr name="rb_drawable_selected" format="reference"/>
        <attr name="starNumber" format="integer"/>
    </declare-styleable>

    <declare-styleable name="LetterIndexBar">
        <attr name="letterTextColor" format="color"/>
        <attr name="letterSelectedColor" format="color"/>
        <attr name="letterTextSize" format="dimension"/>
    </declare-styleable>
</resources>